{
  "name": "dalenguyen",
  "version": "1.2.2",
  "scripts": {
    "ng": "ng",
    "dev": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve --ssl --hmr --port 4201",
    "format:test": "prettier \"src/**/*.{ts,html,md,scss,json}\" --list-different",
    "format:write": "prettier \"src/**/*.{ts,html,md,scss,json}\" --write",
    "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer ./dist/stats.json",
    "start:ssr": "npm run build:ssr && npm run serve:ssr",
    "sentry:build": "ts-node -O '{\"module\": \"commonjs\"}' git.version.ts",
    "build:github": "npm run sentry:build && ng build --prod --base-href 'https://dalenguyen.github.io' --aot --output-hashing=all",
    "deploy:web": "npm run build && ngh --branch=master",
    "deploy:sentry": "sh sentry.sh",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cypress": "cypress open",
    "update:check": "ncu",
    "update:install": "ncu -u && npm i",
    "dev:ssr": "ng run app:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run app:server && cp -a .well-known dist",
    "prerender": "ng run app:prerender",
    "build": "npm run build:ssr",
    "start": "npm run serve:ssr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.1",
    "@angular/cdk": "^11.2.1",
    "@angular/common": "~12.0.1",
    "@angular/compiler": "~12.0.1",
    "@angular/core": "~12.0.1",
    "@angular/forms": "~12.0.1",
    "@angular/material": "^11.2.1",
    "@angular/platform-browser": "~12.0.1",
    "@angular/platform-browser-dynamic": "~12.0.1",
    "@angular/platform-server": "^12.0.1",
    "@angular/router": "~12.0.1",
    "@ng-toolkit/universal": "^8.1.0",
    "@nguniversal/express-engine": "^11.2.0",
    "@sentry/browser": "^6.2.0",
    "@sentry/integrations": "^6.2.0",
    "buttercms": "^1.2.6",
    "express": "^4.17.1",
    "leaflet": "^1.7.1",
    "mapbox-gl": "^2.1.1",
    "rxjs": "~6.6.3",
    "smoothscroll-polyfill": "^0.4.4",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.1",
    "@angular/cli": "~12.0.1",
    "@angular/compiler-cli": "~12.0.1",
    "@angular/language-service": "~12.0.1",
    "@babel/core": "^7.12.17",
    "@cypress/webpack-preprocessor": "^5.6.0",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@nguniversal/builders": "^11.2.0",
    "@types/cypress": "^1.1.3",
    "@types/express": "^4.17.11",
    "@types/jasmine": "~3.6.4",
    "@types/jasminewd2": "~2.0.8",
    "@types/leaflet": "^1.5.23",
    "@types/mapbox-gl": "^2.1.0",
    "@types/node": "^14.14.31",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "codelyzer": "^6.0.1",
    "cypress": "^6.5.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "ts-loader": "^8.0.17",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "4.2.4",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0"
  }
}