{
  "name": "dalenguyen",
  "version": "1.2.2",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --ssl",
    "format:test": "prettier \"src/**/*.{ts,html,md,scss,json}\" --list-different",
    "format:write": "prettier \"src/**/*.{ts,html,md,scss,json}\" --write",
    "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer ./dist/stats.json",
    "start:ssr": "npm run build:ssr && npm run serve:ssr",
    "prebuild": "ts-node -O '{\"module\": \"commonjs\"}' git.version.ts",
    "build:github": "npm run prebuild && ng build --prod --base-href 'https://dalenguyen.github.io' --aot --output-hashing=all",
    "deploy:web": "npm run build && ngh --branch=master",
    "deploy:sentry": "sh sentry.sh",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cypress": "cypress open",
    "update:check": "ncu",
    "update:install": "ncu -u && npm i",
    "dev:ssr": "ng run app:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run app:server && cp -a .well-known dist",
    "prerender": "ng run app:prerender",
    "build": "npm run build:ssr",
    "start": "npm run serve:ssr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.1.0",
    "@angular/cdk": "^11.1.0",
    "@angular/common": "~11.1.0",
    "@angular/compiler": "~11.1.0",
    "@angular/core": "~11.1.0",
    "@angular/forms": "~11.1.0",
    "@angular/material": "^11.1.0",
    "@angular/platform-browser": "~11.1.0",
    "@angular/platform-browser-dynamic": "~11.1.0",
    "@angular/platform-server": "^11.1.0",
    "@angular/router": "~11.1.0",
    "@ng-toolkit/universal": "^8.1.0",
    "@nguniversal/express-engine": "^11.1.0",
    "@sentry/browser": "^5.6.3",
    "@sentry/integrations": "^5.6.1",
    "buttercms": "^1.2.1",
    "express": "^4.17.1",
    "rxjs": "~6.6.3",
    "smoothscroll-polyfill": "^0.4.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1101.1",
    "@angular/cli": "~11.1.1",
    "@angular/compiler-cli": "~11.1.0",
    "@angular/language-service": "~11.1.0",
    "@cypress/webpack-preprocessor": "^4.1.1",
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@nguniversal/builders": "^11.1.0",
    "@types/cypress": "^1.1.3",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.7",
    "@types/node": "^12.11.1",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "codelyzer": "^6.0.0",
    "cypress": "^3.8.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.0.5",
    "protractor": "~7.0.0",
    "ts-loader": "^6.2.1",
    "ts-node": "~8.4.1",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "4.0.5",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.10"
  }
}
