    "chat": {
      "projectType": "application",
      "root": "apps/chat",
      "sourceRoot": "apps/chat/src",
      "prefix": "dalenguyen",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/chat",
