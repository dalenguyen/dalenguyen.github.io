                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/chat/src/environments/environment.ts",
                  "with": "apps/chat/src/environments/environment.prod.ts"
