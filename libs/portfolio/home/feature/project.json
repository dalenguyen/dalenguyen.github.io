{
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/portfolio/home/feature/src",
  "prefix": "dalenguyen",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/portfolio/home/feature/src/**/*.ts",
          "libs/portfolio/home/feature/src/**/*.html"
        ]
      },
      "defaultConfiguration": "local",
      "configurations": {
        "local": {},
        "ci": {
          "outputFile": "libs/portfolio/home/feature/lint.json",
          "format": "json"
        }
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/portfolio/home/feature"
      ],
      "options": {
        "jestConfig": "libs/portfolio/home/feature/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": [
    "type:lib",
    "scope:portfolio"
  ]
}
