{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/openai/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/openai/**/*.ts"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/openai"
      ],
      "options": {
        "jestConfig": "libs/openai/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/openai",
        "tsConfig": "libs/openai/tsconfig.lib.json",
        "packageJson": "libs/openai/package.json",
        "main": "libs/openai/src/index.ts",
        "assets": [
          "libs/openai/*.md"
        ]
      }
    },
    "deploy": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "npm publish dist/libs/openai"
        ]
      }
    }
  },
  "tags": []
}
