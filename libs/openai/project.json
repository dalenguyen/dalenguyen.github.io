{
  "root": "libs/openai",
  "sourceRoot": "libs/openai/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/openai/**/*.ts"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/openai"
      ],
      "options": {
        "jestConfig": "libs/openai/jest.config.js",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/openai",
        "tsConfig": "libs/openai/tsconfig.lib.json",
        "packageJson": "libs/openai/package.json",
        "main": "libs/openai/src/index.ts",
        "assets": [
          "libs/openai/*.md"
        ]
      }
    }
  },
  "tags": []
}
